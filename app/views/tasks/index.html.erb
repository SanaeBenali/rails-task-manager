<h1>My tasks</h1>
<ul>
  <% @tasks.each do |task| %>
    <li><h2><%= link_to "#{task.title}", task_path(task) %></h1></li>
     <%= link_to "Update",
            edit_task_path(task)%>
    <%= link_to "Delete",
            task_path(task),
            method: :delete,
            data: { confirm: "Are you sure?" } %>
  <% end %>
</ul>

<p>Add a new task</p>
